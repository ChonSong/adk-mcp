# Google ADK-Web Configuration
# Copy this file to .env and fill in your values

# Required for Google ADK-Web integration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Optional Google Cloud settings
GOOGLE_CLOUD_LOCATION=us-central1

# Optional ADK-Web model settings
ADK_MODEL_NAME=gemini-1.5-pro
ADK_MAX_TOKENS=1000
ADK_TEMPERATURE=0.7
ADK_ENABLE_STREAMING=true

# Server settings
ADK_SERVER_HOST=0.0.0.0
ADK_SERVER_PORT=8080
ADK_WEBSOCKET_PORT=8081

# Feature toggles
ADK_ENABLE_EXECUTOR=true
ADK_ENABLE_MOCK_SERVICES=true
ADK_ENABLE_GOOGLE_ADK=true